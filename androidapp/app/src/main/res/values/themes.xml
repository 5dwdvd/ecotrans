<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TimTim" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/babypowder</item>
        <item name="colorPrimaryVariant">@color/babypowder</item>
        <item name="colorOnPrimary">@color/babypowder</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/babypowder</item>
        <item name="colorSecondaryVariant">@color/babypowder</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TimTim.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TimTim.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TimTim.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>