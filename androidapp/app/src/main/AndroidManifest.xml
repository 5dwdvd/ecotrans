<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.timtim">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TimTim"
        tools:targetApi="31">
        <activity
            android:name=".Home2"
            android:exported="false"
            android:label="@string/title_activity_home2"
            android:theme="@style/Theme.TimTim.NoActionBar" />
        <activity
            android:name=".Home3"
            android:exported="false"
            android:label="@string/title_activity_home2"
            android:theme="@style/Theme.TimTim.NoActionBar" />
        <activity
            android:name=".MemoryActivity"
            android:exported="false"
            android:label="@string/title_activity_memory"
            android:theme="@style/Theme.TimTim.NoActionBar" />
        <activity
            android:name=".QRCodeActivity"
            android:exported="false"
            android:label="@string/title_activity_qrcode"
            android:theme="@style/Theme.TimTim.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>